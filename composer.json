{
    "name": "mint-soft/form-shaper",
    "description": "Zend Framework 2 module improves various operations on forms, such baking domain model, entities into forms + validation in one line call",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "forms",
        "model",
        "entity",
        "bake",
        "validation",
        "helper"
    ],
    "homepage": "https://github.com/sokool/form-shaper",
    "require": {
        "php": ">=5.4",
        "zendframework/zend-modulemanager": ">= 2.3",
        "zendframework/zend-mvc": ">= 2.3",
        "zendframework/zend-http": ">= 2.3",
        "zendframework/zend-form": ">= 2.3",
        "zendframework/zend-code": ">= 2.3",
        "zendframework/zend-view": ">= 2.3",
        "zendframework/zend-test": ">= 2.3",
        "zendframework/zend-config": ">= 2.3",
        "zendframework/zend-serializer": ">= 2.3",
        "zendframework/zend-log": ">= 2.3",
        "zendframework/zend-authentication": ">= 2.3",
        "zendframework/zend-permissions-rbac": ">= 2.3",
        "zendframework/zend-cache": ">= 2.3",
        "doctrine/annotations": ">=1.1"
    },
    "autoload": {
        "psr-0": {
            "MintSoft\\FormShaper": "src/",
            "FormShaperTest": "tests/"
        }
    }
}
